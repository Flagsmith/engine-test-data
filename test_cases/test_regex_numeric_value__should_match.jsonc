  "context": {
    "environment": {
      "key": "key",
      "name": "Environment"
    },
    "identity": {
      "identifier": "numeric_user",
      "key": "key_numeric_user",
      "traits": {
        "version": 123,
        "old_version": 122
      }
    },
    "segments": {
      "78": {
        "key": "78",
        "name": "segment_regex_numeric_type",
        "rules": [
          {
            "type": "ALL",
            "conditions": [
              {
                "operator": "REGEX",
                "property": "version",
                "value": "\\d*[13579]$"
              }
            ]
          }
        ]
      },
      "79": {
        "key": "79",
